colnames(bam_stats) <- unlist(flagstat_out[[1]]$item)
bam_stats_dt <- cbind(bam_dt,bam_stats)
tsv_file <- './bt2/output/Supplementary_Table_S4_mRNA.tsv'
fwrite(bam_stats_dt,file=tsv_file,sep='\t')
message(tsv_file)
